      //사용 여부 토글 변경 시
      $("#addcmmnUse, #e_cmmnUse").change(function(){
        let nowStr = $(this).next()[0].innerText;
        if(nowStr == "사용함"){
          nowStr = "사용안함";
          $("#addcmmnUse").val(0);
          $("#e_cmmnUse").val(0);
        }else{
          nowStr = "사용함";
          $("#addcmmnUse").val(1);
          $("#e_cmmnUse").val(1);
        }
        $(this).next()[0].innerText = nowStr;
      });


